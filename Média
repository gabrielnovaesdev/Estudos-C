#include <stdio.h>

int main() {
    float nota1, nota2, media;
    
    printf("\nInforme a primeira nota: ");
    scanf("%f", &nota1);
    printf("\nInforme a segunda nota: ");
    scanf("%f", &nota2);
    
    media = (nota1 + nota2) / 2;
    
    printf("\nA media eh: %.1f", media);
    
    if(media >= 7){
        printf("\nO aluno est√° aprovado");
    } else{
        printf("\nO aluno esta reprovado");
    }
    

    
}
