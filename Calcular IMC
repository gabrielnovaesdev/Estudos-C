#include <stdio.h>

int main() {
    double peso, altura, imc;

    printf("Digite o seu peso em Kg: ");
    scanf("%lf", &peso);

    
    printf("Digite a sua altura em metros: ");
    scanf("%lf", &altura);

    
    imc = peso / (altura * altura);

    
    printf("O seu IMC eh: %.2lf\n", imc);
    
    return 0;
   }
