#include <stdio.h>

int main() {
    int num1, num2, num3, num4, maior;

    printf("\nDigite 4 numeros inteiros\n");

    printf("\nDigite o primeiro numero: ");
    scanf("%i", &num1);
    
    printf("\nDigite o segundo numero: ");
    scanf("%i", &num2);
    
    printf("\nDigite o terceiro nnumero: ");
    scanf("%i", &num3);
    
    printf("\nDigite o quarto numero: ");
    scanf("%i", &num4);
    
    maior = num1;
    
    if(num2 > maior){
       maior = num2;
    }
    if(num3 > maior){
       maior = num3;
    }
    if(num4 > maior){
       maior = num4;
    }
    
    printf("\nO maior numero eh: %i\n",maior);
    
    return 0;
   }
